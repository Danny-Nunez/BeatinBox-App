{
  "expo": {
    "name": "beatinbox-app-v2",
    "sdkVersion": "52.0.0",
    "slug": "beatinbox-app-v2",
    "owner": "topspeedwireless",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "beatinbox",
    "extra": {
      "eas": {
        "projectId": "348365da-8efa-4589-83b1-548c4776db7a"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beatinbox.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.163014883667-g0g2t71ei8aroin55ahhtcq491tk2ts8"
        }
      },
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "processing"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.163014883667-g0g2t71ei8aroin55ahhtcq491tk2ts8"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "beatinbox"
            ]
          }
        ],
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you select and upload profile pictures.",
        "NSCameraUsageDescription": "This app needs access to your camera to let you take profile pictures.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beatinbox.app",
      "permissions": [
        "INTERNET",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "beatinbox"
            }
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "googleSignIn": {
          "clientId": "163014883667-kptdg470n3p8ksg658skm1njmpi6fq40.apps.googleusercontent.com"
        }
      }
    },
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you select profile pictures.",
          "cameraPermission": "The app accesses your camera to let you take profile pictures."
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification"
    }
  }
}
